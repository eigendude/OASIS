  <build_depend>libboost-serialization-dev</build_depend>
  <build_depend>libopencv-dev</build_depend>
  <build_depend>libssl-dev</build_depend>
  <build_depend>pangolin</build_depend>

  <url type="website">https://github.com/UZ-SLAMLab/ORB_SLAM3</url>
  <url type="bugtracker">https://github.com/UZ-SLAMLab/ORB_SLAM3/issues</url>
  <url type="repository">https://github.com/UZ-SLAMLab/ORB_SLAM3.git</url>
