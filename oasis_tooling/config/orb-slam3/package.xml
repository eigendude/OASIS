<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>orb_slam3</name>
  <version>1.0.0</version>
  <description>ORB-SLAM3: An Accurate Open-Source Library for Visual, Visual-Inertial and Multi-Map SLAM</description>
  <maintainer email="noone@nowhere.com">SLAMLab - Universidad de Zaragoza</maintainer>
  <license>GPL-3.0-or-later</license>

  <buildtool_depend>cmake</buildtool_depend>

